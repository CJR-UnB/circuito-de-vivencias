<div class="container">
    <h2 class="feedback-title">Feedback</h2>
    <div class="row">
        <div class="col-md-4 col-md-offset-1">
            <h3 class="supervisor_author_title">Supervisor autor: <%= @supervisor_author.name %></h3>
            <h3 class="subject_title">Assunto: <%= @feedback.subject %></h3>
        </div>
    </div>
    <div clas="row">
        <div class="col-md-6 col-md-offset-3">
            <h3 class="content_title">Corpo:</h3>
            <%= simple_format(@feedback.content) %>
        </div>
    </div>
</div>