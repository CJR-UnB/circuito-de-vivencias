<div class='container'>
  <%= form_for @category do |f| %>
    <div class='form-group'>
      <%= f.label(:name, 'Nome') %>
      <%= f.text_field(:name, class: 'form-control') %>
    </div>
    <div class='form_group'>
      <%= f.submit('Enviar', class: 'btn btn-success') %>
    </div>
  <% end %>
</div>
